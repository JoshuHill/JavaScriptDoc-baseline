{{!--
    TODO: removed "attribs" from the start of the heading; looks like we no longer set doc.attribs.
    need to restore that info, perhaps as badges instead of prefix text
--}}
<h id="{{id symbol}}">
    {{#with symbol}}
        {{~#embed 'constructor-prefix'}}{{/embed~}}
        {{~name~}}
        {{~#embed 'signature'}}{{/embed~}}
    {{/with}}
</h>
{{#with symbol}}
    {{#embed 'description'}}{{/embed}}
    {{#embed 'examples'}}{{/embed}}
    {{#embed 'params'}}{{/embed}}
    {{#embed 'properties'}}{{/embed}}

    <dl {{~cssClass '!dl-compact'}}>
        {{#embed 'source-file'}}{{/embed}}
        {{#embed 'augments'}}{{/embed}}
        {{#embed 'details'}}{{/embed}}
        {{#embed 'requires'}}{{/embed}}
        {{#embed 'fires'}}{{/embed}}
        {{#embed 'listens'}}{{/embed}}
        {{#embed 'listeners'}}{{/embed}}
        {{#embed 'throws'}}{{/embed}}
        {{#embed 'returns'}}{{/embed}}
    </dl>
{{/with}}
